{
  "name": "clash-config-manager",
  "productName": "clash-config-manager",
  "bundleId": "fun.magicdawn.clash-config-manager",
  "version": "0.12.3",
  "description": "clash configuration manager",
  "license": "MIT",
  "repository": "magicdawn/clash-config-manager",
  "main": "main/index.js",
  "private": true,
  "author": {
    "name": "magicdawn",
    "email": "magicdawn@qq.com",
    "url": "https://magicdawn.fun"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "electron-builder install-app-deps",
    "dev:main": "pnpm -F main dev",
    "dev:ui": "pnpm -F ui dev",
    "dev:start-main": "electron bundle/development/main/index.js",
    "dev:watch-main": "nodemon bundle/development/main/index.js --exec electron --watch bundle/development/main/",
    "build": "pnpm build:main && pnpm build:ui",
    "build:main": "pnpm -F main build",
    "build:ui": "pnpm -F ui build",
    "dist:pack": "electron-builder --dir",
    "dist:mac": "electron-builder --macos",
    "dist:mac:dmg": "electron-builder --macos dmg",
    "dist": "electron-builder --macos --linux --windows"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "electron-store": "^8.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@magicdawn/prettier-config": "^0.0.2",
    "@swc/core": "^1.2.246",
    "@swc/helpers": "^0.4.11",
    "@types/fancy-log": "^2.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jake": "^0.0.33",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.14",
    "@types/prettier": "2.7.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "electron": "^19.0.15",
    "electron-builder": "^23.5.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.1",
    "fancy-log": "^2.0.0",
    "fs-extra": "^10.1.0",
    "globby": "11",
    "husky": "^8.0.1",
    "jake": "^10.8.5",
    "lint-staged": "^13.0.3",
    "minimist": "^1.2.6",
    "napa": "^3.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "type-fest": "^2.19.0",
    "typescript": "^4.8.2",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.74.0",
    "webpack-merge": "^5.8.0"
  },
  "prettier": "@magicdawn/prettier-config",
  "lint-staged": {
    "*.{js,jsx,less,css,ts,tsx,md}": [
      "prettier --write"
    ]
  }
}
