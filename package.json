    "monaco-editor": "^0.20.x",
    "ms": "^2.1.2",
    "napa": "^3.0.0",
    "promise.ify": "^0.3.0",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-command-palette": "^0.13.0-0",
    "react-dom": "^16.13.1",
