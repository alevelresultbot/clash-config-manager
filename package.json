{
  "name": "clash-config-manager",
  "productName": "clash-config-manager",
  "bundleId": "fun.magicdawn.clash-config-manager",
  "version": "0.8.0",
  "description": "clash configuration manager",
  "license": "MIT",
  "repository": "magicdawn/clash-config-manager",
  "main": "main/index.js",
  "private": true,
  "author": {
    "name": "magicdawn",
    "email": "magicdawn@qq.com",
    "url": "https://magicdawn.fun"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps && husky install",
    "dev:main": "pnpm dev --filter main",
    "dev:ui": "pnpm dev --filter ui",
    "dev:start-main": "electron bundle/development/main/index.js",
    "dev:watch-main": "nodemon bundle/development/main/index.js --exec electron --watch bundle/development/main/",
    "build": "pnpm build:main && pnpm build:ui",
    "build:main": "pnpm build --filter main",
    "build:ui": "pnpm build --filter ui",
    "pack": "electron-builder --dir",
    "dist:mac": "PYTHON_PATH=`pyenv which python2.7` electron-builder --macos",
    "dist": "electron-builder --macos --linux --windows"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "electron-store": "^8.0.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-do-expressions": "^7.16.7",
    "@magicdawn/eslint-config": "latest",
    "@magicdawn/prettier-config": "latest",
    "@types/eslint": "^7.29.0",
    "@types/fancy-log": "^1.3.1",
    "@types/gulp": "^4.0.9",
    "@types/lodash": "^4.14.181",
    "@types/prettier": "2.3.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "electron": "^16.2.2",
    "electron-builder": "^22.14.13",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "fancy-log": "^1.3.3",
    "fs-extra": "^9.0.1",
    "globby": "^11.1.0",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "minimist": "^1.2.5",
    "napa": "^3.0.0",
    "prettier": "latest",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "^2.9.1",
    "webpack": "^5.72.0",
    "webpack-merge": "^5.8.0"
  },
  "packageManager": "pnpm@3.1.1",
  "prettier": "@magicdawn/prettier-config",
  "lint-staged": {
    "*.{js,less,vue,ts,tsx}": [
      "prettier --write"
    ]
  }
}
