{
  "name": "clash-config-manager",
  "productName": "clash-config-manager",
  "bundleId": "fun.magicdawn.clash-config-manager",
  "version": "0.5.2",
  "description": "clash configuration manager",
  "license": "MIT",
  "repository": "magicdawn/clash-config-manager",
  "main": "main/index.js",
  "private": true,
  "author": {
    "name": "magicdawn",
    "email": "magicdawn@qq.com",
    "url": "https://magicdawn.fun"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "electron-builder install-app-deps && husky install",
    "dev:main": "yarn workspace main-process run dev",
    "dev:ui": "yarn workspace ui run dev",
    "dev:start-main": "electron bundle/development/main/index.js",
    "dev:watch-main": "nodemon bundle/development/main/index.js --exec electron --watch bundle/development/main/",
    "build": "yarn build:main && yarn build:ui",
    "build:main": "yarn workspace main-process run build",
    "build:ui": "yarn workspace ui run build",
    "pack": "electron-builder --dir",
    "dist:mac": "electron-builder --macos",
    "dist": "electron-builder --macos --linux --windows"
  },
  "napa": {
    "@magicdawn/x": "magicdawn/x#master"
  },
  "devDependencies": {
    "@babel/plugin-proposal-do-expressions": "^7.10.4",
    "@magicdawn/eslint-config": "latest",
    "@magicdawn/prettier-config": "latest",
    "@poi/plugin-typescript": "^12.3.1",
    "@types/eslint": "^7.2.14",
    "@types/fancy-log": "^1.3.1",
    "@types/gulp": "^4.0.9",
    "@types/lodash": "^4.14.171",
    "@types/poi": "^12.5.2",
    "@types/prettier": "2.3.2",
    "@types/webpack-merge": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.20.6",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "monaco-editor-webpack-plugin": "^2.1.0",
    "napa": "^3.0.0",
    "poi": "latest",
    "prettier": "latest",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.3.5",
    "typescript-plugin-css-modules": "^3.0.1"
  },
  "lint-staged": {
    "*.{js,less,vue,ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
