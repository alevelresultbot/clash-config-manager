{
  "name": "@clash-config-manager/main-process",
  "version": "0.4.0",
  "private": true,
  "productName": "clash-config-manager",
  "bundleId": "fun.magicdawn.clash-config-manager",
  "description": "clash configuration manager",
  "license": "MIT",
  "repository": "magicdawn/clash-config-manager",
  "main": "main/index.js",
  "author": {
    "name": "magicdawn",
    "email": "magicdawn@qq.com",
    "url": "https://magicdawn.fun"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "_install": "napa",
    "_postinstall": "electron-builder install-app-deps",
    "dev:ui": "poi -s -r ts-node/register/transpile-only",
    "dev:main": "poi --config poi-main.config.js --watch",
    "dev:start-main": "electron bundle/development/main/index.js",
    "dev:watch-main": "nodemon bundle/development/main/index.js --exec electron --watch bundle/development/main/",
    "build": "yarn build:ui && yarn build:main",
    "build:ui": "poi --prod -r ts-node/register/transpile-only",
    "build:main": "poi --prod --config poi-main.config.js",
    "pack": "electron-builder --dir",
    "dist:mac": "electron-builder --macos",
    "dist": "electron-builder --macos --linux --windows"
  },
  "napa": {
    "@magicdawn/x": "magicdawn/x#master"
  },
  "dependencies": {
    "bytes": "^3.1.0",
    "debug": "^4.3.1",
    "dl-vampire": "^0.5.0",
    "electron-context-menu": "^2.3.1",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.3.1",
    "electron-store": "^6.0.1",
    "electron-unhandled": "^3.0.0",
    "electron-updater": "^4.0.6",
    "electron-util": "^0.13.0",
    "env-paths": "^2.2.0",
    "execa": "^5.0.0",
    "fix-path": "^3.0.0",
    "fs-extra": "^9.0.1",
    "js-yaml": "^3.14.1",
    "lodash": "^4.17.20",
    "memoize-one": "^5.1.1",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "ms": "^2.1.3",
    "promise.ify": "^0.4.0",
    "spawn-sync": "^2.0.0",
    "umi-request": "^1.3.5",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@poi/plugin-typescript": "^12.3.1",
    "@types/bytes": "^3.1.1",
    "@types/debug": "^4.1.6",
    "@types/fancy-log": "^1.3.1",
    "@types/fs-extra": "^9.0.12",
    "@types/js-yaml": "^4.0.2",
    "@types/lodash": "^4.14.171",
    "@types/minimist": "^1.2.2",
    "@types/ms": "^0.7.31",
    "@types/yargs": "^17.0.2",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "fancy-log": "^1.3.3",
    "globby": "^11.0.1",
    "minimist": "^1.2.5"
  }
}
